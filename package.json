{
  "name": "mg",
  "description": "MoyGrafik",
  "author": "CSSSR <sales@csssr.io> (https://github.com/CSSSR)",
  "private": true,
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/CSSSR/mg"
  },
  "homepage": "",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "hot",
    "react-hot-reloader",
    "redux",
    "babel"
  ],
  "main": "bin/server.js",
  "scripts": {
    "start": "concurrently --kill-others \"npm run start-prod\"",
    "start-prod": "better-npm-run start-prod",
    "start-prod-mg": "better-npm-run start-prod-mg",
    "start-prod-api": "better-npm-run start-prod-api",
    "start-dev-api": "better-npm-run start-dev-api",
    "build": "concurrently --kill-others \"better-npm-run build\"",
    "lint": "eslint -c .eslintrc src api --ext js,jsx",
    "lint-fix": "eslint -c .eslintrc src api --ext js,jsx --fix",
    "start-dev": "better-npm-run start-dev",
    "start-dev-render-on-client": "better-npm-run start-dev-render-on-client",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\" ",
    "dev-fast": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev-render-on-client\"",
    "test": "better-npm-run test",
    "test-node": "./node_modules/mocha/bin/mocha $(find api -name '*.test.js') --compilers js:babel-core/register",
    "test-node-watch": "./node_modules/mocha/bin/mocha $(find api -name '*.test.js') --compilers js:babel-core/register --watch",
    "storybook": "start-storybook -p 9001",
    "build-storybook": "better-npm-run build-storybook"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node --harmony ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 58695,
        "APIPORT": 34032
      }
    },
    "start-prod-mg": {
      "command": "node --harmony ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "__DISABLE_SSR__": true,
        "PREFIX": "/web",
        "PORT": 33068,
        "APIPORT": 34032
      }
    },
    "start-prod-api": {
      "command": "node $NODE_DEBUG_OPTION --harmony ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "production",
        "PORT": 58695,
        "APIPORT": 34032
      }
    },
    "start-dev-api": {
      "command": "./node_modules/.bin/supervisor -w api --harmony ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "development",
        "APIPORT": 3031
      }
    },
    "start-dev": {
      "command": "node $NODE_DEBUG_OPTION ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "PREFIX": "",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3031
      }
    },
    "start-dev-render-on-client": {
      "command": "node $NODE_DEBUG_OPTION ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "__DISABLE_SSR__": true,
        "NODE_ENV": "development",
        "HOST": "localhost",
        "APIHOST": "localhost",
        "PORT": 3000,
        "APIPORT": 3031
      }
    },
    "watch-client": {
      "command": "node ./bin/webpack.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src/"
      }
    },
    "build": {
      "command": "webpack --color --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production",
        "PREFIX": "/web"
      }
    },
    "build-storybook": {
      "command": "build-storybook -o static/__components",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "jest --coverage",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "blueprints",
      "api"
    ],
    "transform": {
      ".*": "<rootDir>/webpack/hook-test.js"
    },
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/*.story.js",
      "!**/*.mock.js",
      "!**/index.js",
      "!./webpack/**"
    ]
  },
